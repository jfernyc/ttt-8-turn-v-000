#!/usr/bin/env ruby
required_relative '../lib/turn'

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
 
puts "Welcome to Tic Tac Toe!"
display_board(board)


def board = [" ", " ", " "," ", " "," "," ", " ", " "]

puts "Welcome to Tic Tac Toe!"

puts "Please enter 1-9:"

input = gets.strip

index =  input_to_index(input)

move(board, index)

display_board(board)

end
 end